{
  "expo": {
    "name": "GOLFIN",
    "slug": "GOLFIN",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/golf.png",
    "scheme": "golfin",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.GOLFIN"
    },
    "splash": {
      "resizeMode": "contain",
      "image": "./assets/images/BG IMG GLF.png",
      "backgroundColor": "#ffffff"
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "73b0f30f-dd05-4ca2-ad81-c079c1f71cf3"
      }
    },
    "owner": "iayden"
  }
}
